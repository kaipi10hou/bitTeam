<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
      PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.profile">
	<resultMap id="profileResult" type="profileVO">
		<result property="id" column="name" />
		<result property="profileImage" column="profileImage" />
		<result property="jagisogae" column="jagisogae" />
	</resultMap>


  <insert id="insertNewProfile"  parameterType="java.util.Map">
		<![CDATA[
			INSERT into kakao_profile(id, profileImage, jagisogae)
			VALUES(#{id}, #{profileImage}, #{jagisogae})
		]]>      
	</insert>
	

</mapper>